{
    "name": "pauseshop-server",
    "version": "0.0.0",
    "description": "PauseShop Backend Server",
    "private": true,
    "scripts": {
        "prebuild": "ts-node scripts/generate-icon-categories.ts",
        "build": "tsc",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "start": "node dist/index.js",
