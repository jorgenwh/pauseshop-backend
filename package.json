    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "jest": "^29.5.0",
        "rimraf": "^5.0.0",
        "ts-jest": "^29.1.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@google/genai": "^1.3.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "multer": "^1.4.5-lts.1",
        "openai": "^4.0.0"
